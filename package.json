{
  "name": "marketplace-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vite": "vite",
    "dev": "concurrently --kill-others \"vite\" \"tcm --watch .\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix . --ext .js,.ts,.tsx",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "css-modules:watch": "tcm --watch .",
    "css-modules:generate": "tcm .",
    "css-modules-check:up-to-date-types": "tcm --listDifferent ."
  },
  "dependencies": {
    "@million/lint": "^0.0.73",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tanstack/react-query": "^5.28.6",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dompurify": "^3.0.9",
    "embla-carousel-react": "^8.0.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "lucide-react": "^0.360.0",
    "million": "^3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.30.3",
    "typed-css-modules": "^0.9.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.28.6",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-newline-destructuring": "^1.2.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.38",
    "stylelint": "^16.3.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
